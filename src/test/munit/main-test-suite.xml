<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="main-test-suite.xml"/>
	<munit:test name="main-test-suite-check-in-by-pnrTest" doc:id="5446f941-2dc5-43c9-9d8a-cbfefad2896b" description="Test">
		<munit:behavior >
			<flow-ref doc:name="all-dependencies-mocks" doc:id="0f2bcbdc-d01c-4b1c-8587-468436bc4063" name="all-dependencies-mocks"/>
			<munit-tools:spy doc:name="check-in-flights-management" doc:id="74309bce-ed1f-43f3-b697-a2524fe0b4cc" processor="flow-ref">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="check-in-flights-management" attributeName="doc:name" />
				</munit-tools:with-attributes>
				<munit-tools:before-call >
					<munit-tools:assert-equals doc:name="Assert equals" doc:id="75defbca-0d99-4017-abcb-87d49f5c2e38" actual="#[payload]" expected="#[vars.checkInPayload]" message="The actual request payload DOES NOT match the expected request payload!"/>
				</munit-tools:before-call>
				<munit-tools:after-call >
					<munit-tools:assert-equals doc:name="Assert equals" doc:id="93933289-366c-4f91-b90b-05029add9dba" actual="#[payload]" expected="#[dwl::TestData::mockPayloadFMS]" message="The actual response payload DOES NOT match the expected response payload!"/>
				</munit-tools:after-call>
			</munit-tools:spy>
		</munit:behavior>
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="0faa027d-88d1-426d-b311-f7b1dacbd119" >
				<munit:payload value="#[dwl::TestData::requestPayload]" />
				<munit:variables >
					<munit:variable key="PNR" value="#[ dwl::TestData::testPNR ]" />
				</munit:variables>
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to check-in-by-pnr" doc:id="da348c8d-1f32-4e2e-9b07-afd3377b7d3b" name="check-in-by-pnr"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="1dee924b-37b3-4a2d-a30c-b2d8d8f49f6f" actual="#[payload]" expected="#[dwl::TestData::responsePayload]" message="The actual flow response payload DOES NOT match the expected flow response payload!"/>
			<flow-ref doc:name="Flow Reference" doc:id="1e018852-320b-4e38-a40e-8cfe1fc0f072" name="all-dependecies-verify"/>
		</munit:validation>
	</munit:test>
	<munit:test name="main-test-suite-payment-approval-by-pnrTest" doc:id="df8eda94-0178-4629-aa66-4a2f2953c786" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to payment-approval-by-pnr" doc:id="491f378f-7dac-4ef6-ac34-c4c7864cd582" name="payment-approval-by-pnr"/>
		</munit:execution>
	</munit:test>
	<sub-flow name="all-dependencies-mocks" doc:id="623feee2-6cae-4c5f-a277-028e5c8c5886" >
		<munit-tools:mock-when doc:name="check-in-flights-management" doc:id="185cfeb8-3d61-4a06-8ae1-8c7c92b61782" processor="flow-ref">
				<munit-tools:with-attributes>
					<munit-tools:with-attribute whereValue="check-in-flights-management" attributeName="doc:name" />
				</munit-tools:with-attributes>
				<munit-tools:then-return>
					<munit-tools:payload value="#[dwl::TestData::mockPayloadFMS]" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		<munit-tools:mock-when doc:name="register-passenger-data" doc:id="47463646-d309-4e47-87be-7581add719b4" processor="flow-ref" >
			<munit-tools:with-attributes >
				<munit-tools:with-attribute whereValue="register-passenger-data" attributeName="doc:name" />
			</munit-tools:with-attributes>
			<munit-tools:then-return >
				<munit-tools:payload value="#[dwl::TestData::mockPayloadRPD]" />
			</munit-tools:then-return>
		</munit-tools:mock-when>
		<munit-tools:mock-when doc:name="create-payment-for-bags" doc:id="3dcd8b74-97d0-4024-bf36-15b1dd214766" processor="flow-ref" >
			<munit-tools:with-attributes >
				<munit-tools:with-attribute whereValue="create-payment-for-bags" attributeName="doc:name" />
			</munit-tools:with-attributes>
			<munit-tools:then-return >
				<munit-tools:payload value="#[dwl::TestData::mockPayloadCPB]" />
			</munit-tools:then-return>
		</munit-tools:mock-when>
	</sub-flow>
	<sub-flow name="all-dependecies-verify" doc:id="d9f097af-b289-4216-8964-cc2672b21bf4" >
		<munit-tools:verify-call doc:name="check-in-flights-management" doc:id="d180243d-78c6-4e65-ae82-afbc8bf2cd6f" processor="flow-ref" times="1">
				<munit-tools:with-attributes>
					<munit-tools:with-attribute whereValue="check-in-flights-management" attributeName="doc:name" />
				</munit-tools:with-attributes>
			</munit-tools:verify-call>
		<munit-tools:verify-call doc:name="register-passenger-data" doc:id="db1dec0c-53c2-464d-b77c-afb9ad150419" processor="flow-ref" atMost="1">
				<munit-tools:with-attributes>
					<munit-tools:with-attribute whereValue="register-passenger-data" attributeName="doc:name" />
				</munit-tools:with-attributes>
			</munit-tools:verify-call>
		<munit-tools:verify-call doc:name="create-payment-for-bags" doc:id="ccba7c14-b30d-455d-ae92-df5c9db0e5ce" processor="flow-ref" atLeast="1">
			<munit-tools:with-attributes >
				<munit-tools:with-attribute whereValue="create-payment-for-bags" attributeName="doc:name" />
			</munit-tools:with-attributes>
		</munit-tools:verify-call>
	</sub-flow>


</mule>
